{
  "name": "@viastud/server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node --conditions=production bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    "./api": "./.adonisjs/api.ts",
    "./routers/*": "./app/routers/*.js",
    "./services/*": "./app/services/*.js"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#routers/*": "./app/routers/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#config/*": "./config/*.js"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.3.0",
    "@adonisjs/core": "^6.17.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/lucid": "^21.6.1",
    "@adonisjs/session": "^7.5.0",
    "@aws-sdk/client-s3": "^3.723.0",
    "@aws-sdk/s3-request-presigner": "^3.723.0",
    "@rlanz/sentry": "^0.1.0",
    "@sentry/node": "^8.47.0",
    "@trpc/server": "catalog:",
    "@types/bcrypt": "^6.0.0",
    "@viastud/utils": "workspace:*",
    "@vinejs/vine": "^3.0.0",
    "axios": "^1.7.9",
    "bcrypt": "^6.0.0",
    "dayjs": "^1.11.13",
    "jose": "^5.10.0",
    "json": "^11.0.0",
    "luxon": "^3.5.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "stripe": "^17.5.0",
    "twilio": "^5.8.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/api-client": "^2.0.4",
    "@japa/assert": "^4.0.1",
    "@japa/expect": "^3.0.4",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^4.2.0",
    "@swc/core": "^1.10.4",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.5",
    "hot-hook": "^0.4.0",
    "japa": "^4.0.0",
    "openapi-types": "^12.1.3",
    "pino-pretty": "^13.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "files": [
    ".env",
    "build",
    "ecosystem.config.cjs"
  ]
}
